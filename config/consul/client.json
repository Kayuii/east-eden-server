{
    "services": [
    {
        "id": "client_1",
        "name": "client",
        "tags": ["client"],
        "address": "192.168.1.137",
        "port": 80,
        "check": {
          "id": "http_check_client1",
          "name": "HTTP API on port 80",
          "http": "http://192.168.1.137:80/health_check",
          "method": "GET",
          "interval": "10s",
          "timeout": "3s"
        }
    }
    ],

    "watches": [
    {
        "type": "key",
        "key": "csserver",
        "handler_type": "http",
        "http_handler_config": {
            "path": "http://192.168.1.137:80/watch_key",
            "method": "GET",
            "timeout": "10s",
            "tls_skip_verify": false
        }
    },

    {
        "type": "service",
        "service": "client",
        "passingonly": true,
        "handler_type": "http",
        "http_handler_config": {
            "path": "http://192.168.1.137:80/watch_service",
            "method": "GET",
            "timeout": "10s",
            "tls_skip_verify": false
        }
    }

  ]
}
